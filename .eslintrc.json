{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "trybe-backend",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "eslint-disable": 0,
        "max-lines-per-function": ["error", 180],
        "complexity": ["error", 6],
        "max-len": ["error", { "code": 157 }]
    }
}
